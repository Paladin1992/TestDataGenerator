{
    @Html.ActionLink(Messages.Link_Caption_Logout, "Logout", "Account")

    @Html.ActionLink(Messages.Link_Caption_Setup_List, "List", "Setup")
    @*@Html.ActionLink(Messages.Link_Caption_Setup_Create, "Create", "Setup")*@
    @*@Html.ActionLink(Messages.Link_Caption_Setup_Edit, "Edit", "Setup")*@
}
else
{
    @Html.ActionLink(Messages.Link_Caption_Register, "Register", "Account")
    @Html.ActionLink(Messages.Link_Caption_Login, "Login", "Account")
    @Html.ActionLink(Messages.Link_Caption_ForgottenPassword, "ForgottenPassword", "Account")
}

